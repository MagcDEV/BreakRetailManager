{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/css/*",
      "/js/*",
      "/manifest.webmanifest",
      "/service-worker.js",
      "/service-worker.published.js",
      "/_framework/*",
      "/favicon.png",
      "/icon-*.png"
    ]
  },
  "globalHeaders": {
    "Blazor-Environment": "Production",
    "X-Content-Type-Options": "nosniff"
  }
}
